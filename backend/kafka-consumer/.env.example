# backend/kafka-consumer/.env.example
# Environment expected by the Kafka consumer/leaderboard worker.

MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.example.mongodb.net/mydb?retryWrites=true&w=majority
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# If the consumer writes to a cache collection with a specific id
LEADERBOARD_CACHE_ID=top5

# Optional: logging/config
# LOG_LEVEL=info
